    "vusion",
    "cli",
    "spa",
    "webapp"
  ],
  "tags": [
    "vusion",
    "cli",
    "spa",
    "webapp"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
