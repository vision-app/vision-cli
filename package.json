{
  "name": "vusion-cli",
  "description": "CLI for Vusion Projects, including Application and UI Library.",
  "version": "0.5.4",
  "author": "Rainfore <rainforest92@126.com>",
  "preferGlobal": true,
  "bin": {
    "vusion": "bin/vusion",
    "vusion-dev": "bin/vusion-dev",
    "vusion-build": "bin/vusion-build"
  },
  "scripts": {
    "precommit": "bash node_modules/vusion-hooks/pre-commit",
    "test": "npm-run-all test-*",
    "test-init": "nyc --check-coverage mocha test/init.test.js --timeout 60000 --require babel-register",
    "test-dev": "cross-env TEST=true nyc --check-coverage  mocha test/dev.test.js --timeout 120000 --require babel-register",
    "test-build": "nyc --check-coverage mocha test/build.test.js --timeout 120000 --require babel-register",
    "test-svg-sprite": "nyc --check-coverage mocha test/svg-sprite.test.js --timeout 60000 --require babel-register",
    "test-transform": "nyc --check-coverage mocha test/transform.test.js --timeout 60000 --require babel-register"
  },
  "repository": "vusion/vusion-cli",
  "homepage": "http://vusion.github.io",
  "bugs": {
    "url": "http://github.com/vusion/vusion-cli/issues"
  },
  "license": "MIT",
  "keywords": [
    "vusion",
    "cli",
    "spa",
    "webapp"
  ],
  "tags": [
    "vusion",
    "cli",
    "spa",
    "webapp"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
