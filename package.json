{
  "name": "vusion-cli",
  "description": "CLI for Vusion Projects, including Application and UI Library.",
  "version": "0.4.4",
  "author": "Rainfore <rainforest92@126.com>",
  "preferGlobal": true,
  "bin": {
    "vusion": "bin/vusion"
  },
  "scripts": {
    "precommit": "bash node_modules/vusion-hooks/pre-commit",
    "test": "npm run test-init && npm run test-dev && npm run test-build",
    "test-init": "mocha test/test-init.js --timeout 60000 --require babel-register",
    "test-dev": "cross-env TEST=true mocha test/test-dev.js --timeout 60000 --require babel-register",
    "test-build": "mocha test/test-build.js --timeout 60000 --require babel-register"
  },
  "repository": "vusion/vusion-cli",
  "homepage": "http://vusion.github.io",
