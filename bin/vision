#!/usr/bin/env node

'use strict';

require('../src/checkVersions')();
const fs = require('fs');
const pkg = require('../package.json');

/**
 * Parse Commands
 */
const program = require('commander');
program
    .usage('<task> [options]')
    .version(pkg.version)
    //  .command('init [options] <repo>', 'init a repo')
    .command('dev [options]', 'dev project')
    .command('build [options]', 'build project')
    .parse(process.argv);
