#!/usr/bin/env node

'use strict';

/**
 * Parse Commands
 */
const program = require('commander');
program
    .usage('[options]')
    .parse(process.argv);

process.env.NODE_ENV = 'development';
global.visionConfig = {
    docsPath: './docs',
};

const port = process.env.PORT || 5000; // config.dev.port;

const dev = require('../src/dev');
dev(require('../config/vision-ui'), port);
