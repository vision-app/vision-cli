#!/usr/bin/env node

'use strict';

/**
 * Parse Commands
 */
const program = require('commander');
program
    .usage('[options]')
    .parse(process.argv);

process.env.NODE_ENV = 'development';
global.visionConfig = Object.assign({
    docsPath: './docs',
}, require('../src/getVisionConfig'));

const dev = require('../src/dev');
const port = process.env.PORT || 5000; // config.dev.port;
dev(require('../config/vision-ui'), port);
